layout: _Layout
isHome: true
Section: "/blog"

ArchiveSources: "blog/posts/*"
ArchiveDestination: >
  => GetInt("Index") <= 1 ? $"index.html" : $"archive/{GetInt("Index")}.html"
ArchivePageSize: 6
ArchiveOrderKey: Published
ArchiveOrderDescending: true
ArchiveTitle: => GetString("Title")
NoChildPages: true
---
@await Html.PartialAsync("blog/_posts.cshtml", Document)